{
  "name": "bigcommerce-oauth",
  "version": "0.1.0",
  "description": "Framework-agnostic OAuth utilities for BigCommerce apps",
  "main": "dist/index.cjs",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./gateways/bigcommerce": {
      "types": "./dist/gateways/BigCommerce/index.d.ts",
      "import": "./dist/gateways/BigCommerce/index.js",
      "require": "./dist/gateways/BigCommerce/index.cjs"
    },
    "./use-cases/install": {
      "types": "./dist/use-cases/install/index.d.ts",
      "import": "./dist/use-cases/install/index.js",
      "require": "./dist/use-cases/install/index.cjs"
    },
    "./use-cases/load": {
      "types": "./dist/use-cases/load/index.d.ts",
      "import": "./dist/use-cases/load/index.js",
      "require": "./dist/use-cases/load/index.cjs"
    },
    "./use-cases/uninstall": {
      "types": "./dist/use-cases/uninstall/index.d.ts",
      "import": "./dist/use-cases/uninstall/index.js",
      "require": "./dist/use-cases/uninstall/index.cjs"
    },
    "./use-cases/remove-user": {
      "types": "./dist/use-cases/remove-user/index.d.ts",
      "import": "./dist/use-cases/remove-user/index.js",
      "require": "./dist/use-cases/remove-user/index.cjs"
    },
    "./use-cases/shared": {
      "types": "./dist/use-cases/shared/index.d.ts",
      "import": "./dist/use-cases/shared/index.js",
      "require": "./dist/use-cases/shared/index.cjs"
    },
    "./value-objects": {
      "types": "./dist/use-cases/value-objects/index.d.ts",
      "import": "./dist/use-cases/value-objects/index.js",
      "require": "./dist/use-cases/value-objects/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
